<%- include("part/header.ejs") %>
<form action="/patient/<%=data.id%>/edit" method="POST">
  <label for="fname">First name:</label><br />
  <input
    type="text"
    id="fname"
    name="first_name"
    value="<%= data.first_name %> "
  /><br />
  <br />
  <label for="lname">Last name:</label><br />
  <input
    type="text"
    id="lname"
    name="last_name"
    value="<%= data.last_name %>"
  /><br /><br />
  <label for="age">Age:</label><br />
  <input
    type="number"
    id="age"
    name="age"
    value="<%= data.age %>"
  /><br /><br />
  <label for="age">Illness:</label><br />
  <input
    type="text"
    id="illness"
    name="illness"
    value="<%= data.illness %>"
  /><br /><br />
  <input type="submit" value="Submit" />
</form>
<!-- Optional JavaScript; choose one of the two! -->
<%- include("part/footer.ejs") %>
